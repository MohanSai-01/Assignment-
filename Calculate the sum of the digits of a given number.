import java.util.Scanner;
public class sample{
public static void main(String[] args){
Scanner sc = new Scanner(System.in);
System.out.println("Enter the number :");
int a = sc.nextInt();
int rev=0;
int sum=0;
while(a>0){
rev=a%10;
sum=sum+rev;
a=a/10;
}
System.out.println("The sum of digits is :"+sum);
}
}
