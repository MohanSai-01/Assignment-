import java.util.Scanner;
public class sample{
public static void main(String[] args){
Scanner sc = new Scanner(System.in);
System.out.println("Enter the value:");
int a = sc.nextInt();
int count = 0;
if (a == 0){
count = 1;
}
else{
while (a > 0){
a = a/10;
count++;
}
}
System.out.println("Number of digits: " + count);
}
}
